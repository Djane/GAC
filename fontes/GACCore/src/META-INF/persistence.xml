<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0"
	xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="GACCore" transaction-type="RESOURCE_LOCAL">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>

		<class>br.com.sw2.gac.modelo.Acionamento</class>
		<class>br.com.sw2.gac.modelo.Cid</class>
		<class>br.com.sw2.gac.modelo.Cliente</class>
		<class>br.com.sw2.gac.modelo.Contato</class>
		<class>br.com.sw2.gac.modelo.Contrato</class>
		<class>br.com.sw2.gac.modelo.Dispositivo</class>
		<class>br.com.sw2.gac.modelo.Formacomunica</class>
		<class>br.com.sw2.gac.modelo.Monitoramento</class>
		<class>br.com.sw2.gac.modelo.Ocorrencia</class>
		<class>br.com.sw2.gac.modelo.Pacoteservico</class>
		<class>br.com.sw2.gac.modelo.Parametro</class>
		<class>br.com.sw2.gac.modelo.Script</class>
		<class>br.com.sw2.gac.modelo.Sms</class>
		<class>br.com.sw2.gac.modelo.Tipodoenca</class>
		<class>br.com.sw2.gac.modelo.Tratamento</class>
		<class>br.com.sw2.gac.modelo.Usuario</class>
		<properties>
			<property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver" />
			<property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/dbGAC" />
			<property name="javax.persistence.jdbc.user" value="root" />
			<property name="javax.persistence.jdbc.password" value="root" />
			<property name="eclipselink.logging.level" value="FINE" />
		</properties>
	</persistence-unit>
</persistence>
